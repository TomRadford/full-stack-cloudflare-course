/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "data-service",
	"main": "src/index.ts",
	"compatibility_date": "2025-06-17",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_id": "c77c4213-31e5-4ca7-bdce-aadeb644db5b",
			"remote": true
		}
	],
	"kv_namespaces": [{ "binding": "CACHE", "id": "0b145f63f8d246b2b036d82eb3944c1d", "remote": true }],
	"queues": {
		"consumers": [
			{ "queue": "smart-links-data-queue-stage", "dead_letter_queue": "smart-links-dead-letter-stage", "retry_delay": 0 },
			{ "queue": "smart-links-dead-letter-stage" }
		],
		"producers": [{ "binding": "QUEUE", "queue": "smart-links-data-queue-stage" }]
	},
	"workflows": [
		{
			"binding": "DESTINATION_EVALUATION_WORKFLOW",
			"name": "destination-evaluation-workflow",
			"class_name": "DestinationEvaluationWorkflow"
		}
	],
	"browser": {
		"binding": "VIRTUAL_BROWSER"
	},
	"ai": {
		"binding": "AI"
	},
	"r2_buckets": [
		{
			"binding": "BUCKET",
			"bucket_name": "smart-links-eval-stage"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"name": "EVALUATION_SCHEDULER",
				"class_name": "EvaluationScheduler"
			},
			{ "name": "LINK_CLICK_TRACKER_OBJECT", "class_name": "LinkClickTracker" }
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_classes": ["EvaluationScheduler"]
		},
		{
			"tag": "v2",
			"new_classes": ["LinkClickTracker"]
		}
	]
}
